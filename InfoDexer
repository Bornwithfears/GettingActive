-- Script:

local function GNNPZM_fake_script() -- InfoDexer.Dexer 
	local script = Instance.new('LocalScript', InfoDexer)

	local Front = script.Parent.Back.Front
	local function autofill(arg)
		for i,v in pairs(game:service"Players":GetChildren()) do
			if v.Name:lower():find(arg:lower()) or v.DisplayName:lower():find(arg:lower()) then
				return v.Name
			end
		end
	end

	Front.TextBox.FocusLost:Connect(function()
		local plr = autofill(Front.TextBox.Text)
		if plr then
			getgenv().Player = game:service"Players"[plr]
			Front.ImageLabel.Image = game.Players:GetUserThumbnailAsync(Player.UserId, Enum.ThumbnailType.AvatarThumbnail, Enum.ThumbnailSize.Size420x420)
			Front.Username.Text = Player.Name
		end
	end)
	Front.TextButton.MouseButton1Click:Connect(function()
		local function DexInfo()
			function comma_value(amount)
				local formatted = amount
				while true do  
					formatted, k = string.gsub(formatted, "^(-?%d+)(%d%d%d)", '%1,%2')
					if (k==0) then
						break
					end
				end
				return formatted
			end
			local Credits = "\n\n || Made by - <@!537034346712793110> ||"
			local Title = "# Player ("..Player.Name..") Info Dexed!\n"
			local Outfit = "# Outfit\n"
			local DisplaySTR = "**Display, **\n```"..tostring(Player.DisplayName).."```\n"
			local UserSTR = "**Username, **\n```"..tostring(Player.Name).."```\n"
			local UserIdSTR = "**UserId, **\n```"..tostring(Player.UserId).."```\n"
			local DHC = "**Dahood Cash, **\n```$"..tostring(comma_value(Player.DataFolder.Currency.Value)).."```\n"
			local Wanted = "**Wanted, **\n```$"..tostring(comma_value(Player.DataFolder.Information.Wanted.Value)).."```\n"
			local BackPackString = ""
			for _,v in pairs(Player.Backpack:GetChildren()) do
				BackPackString = BackPackString..v.Name..", "
			end
			local BackPack = "**BackPack, **\n```"..tostring(BackPackString).."```\n"
			local ShirtCat = "https://www.roblox.com/catalog/"..tostring(Player.Character.Humanoid.HumanoidDescription.Shirt)
			local pantsCat = "https://www.roblox.com/catalog/"..tostring(Player.Character.Humanoid.HumanoidDescription.Pants)
			local Shirt = "**Shirt, **\n\n"..ShirtCat.."\n\n"
			local Pants = "**Pants, **\n\n"..pantsCat.."\n"
			local response = request({ Url = Value.WebHook,
				Method = 'POST',
				Headers = {
					['Content-Type'] = 'application/json' 
				},
				Body = game:GetService('HttpService'):JSONEncode({
					["embeds"] = {
						{
							["title"] = "",
							["description"] = Title..DisplaySTR..UserSTR..UserIdSTR..DHC..Wanted..BackPack..Outfit..Shirt..Pants..Credits,
							["color"] = 0xf50202,
						}
					}
				}) 
			});
			BackPackString = ""
		end
		DexInfo()
	end)
end
coroutine.wrap(GNNPZM_fake_script)()
